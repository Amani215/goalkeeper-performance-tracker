          },
          "queryText": "SELECT  g.name as \"Goalkeeper\", AVG(gp.grade) as \"Grade Average\"\nFROM    goalkeeper g\nINNER JOIN(\n  SELECT  mm.main_goalkeeper_id, mm.grade\n  FROM    match_monitoring mm\n          INNER JOIN\n          (\n              SELECT id\n              FROM match\n              where date <= date('now')\n              ORDER BY \"date\" DESC\n              limit 3\n          ) m ON  mm.match_id = m.id\n)gp ON gp.main_goalkeeper_id = g.id\ngroup by g.id\nORDER BY AVG(gp.grade) DESC\nlimit 5",
          "queryType": "table",
          "rawQueryText": "SELECT  g.name as \"Goalkeeper\", AVG(gp.grade) as \"Grade Average\"\nFROM    goalkeeper g\nINNER JOIN(\n  SELECT  mm.main_goalkeeper_id, mm.grade\n  FROM    match_monitoring mm\n          INNER JOIN\n          (\n              SELECT id\n              FROM match\n              where date <= date('now')\n              ORDER BY \"date\" DESC\n              limit 3\n          ) m ON  mm.match_id = m.id\n)gp ON gp.main_goalkeeper_id = g.id\ngroup by g.id\nORDER BY AVG(gp.grade) DESC\nlimit 5",
          "refId": "A",
          "timeColumns": ["time", "ts"]
        }
      ],
